{
    "name": "hbrawnak/limitr",
    "description": "A middleware-agnostic rate limitr with dynamic rules and storage backends support",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hbrawnak\\Limitr\\": "src/",
            "Hbrawnak\\Limitr\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Md Habibur Rahman",
            "email": "hbrawnak@email.com"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "suggest": {
        "ext-redis": "Required for Redis storage support"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
